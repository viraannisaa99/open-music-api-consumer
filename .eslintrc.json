{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": 0,
        "max-len": "off",
        "no-unused-vars": "off",
        "padded-blocks": "off",
        "no-undef": "off",
        "object-curly-newline": "off",
        "no-underscore-dangle": "off",
        "comma-dangle": "off",
        "allowMultiplePropertiesPerLine": true
    }
}
